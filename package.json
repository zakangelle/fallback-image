    "url": "https://github.com/zakangelle/fallback-image"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.0.0",
    "nodemock": "^0.3.4",
    "tape": "^3.4.0"
  },
  "bugs": {
    "url": "https://github.com/zakangelle/fallback-image/issues"
  },
  "homepage": "https://github.com/zakangelle/fallback-image",
  "directories": {
    "test": "test"
  }
